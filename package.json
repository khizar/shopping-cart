{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate with the most basic setup that i like to start with.",
  "main": "index.js",
  "scripts": {
    "clean-install": "rm -rf node_modules && npm install",
    "build": "webpack -p --config webpack/production.config.js",
    "start": "webpack-dev-server --inline --hot --config webpack/dev.config.js",
    "test": "mocha --opts ./app/test/mocha.opts",
    "test:watch": "mocha --opts ./app/test/mocha.opts --watch",
    "precommit": "lint-staged",
    "pretify": "node_modules/.bin/prettier --print-width 100 --single-quote --tab-width 4 --write 'app/**/*.{js,jsx}'"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "node_modules/.bin/prettier --print-width 100 --single-quote --tab-width 4 --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com:khizar/react-boilerplate.git"
  },
  "author": "khizar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khizar/react-boilerplate/issues"
  },
  "dependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-css-modules": "^4.1.0",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-thunk": "^2.2.0",
    "seamless-immutable": "^7.1.2",
    "webpack": "^2.7.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "chai": "^4.1.2",
    "css-loader": "^0.28.5",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-15": "^1.0.3",
    "fetch-mock": "^5.13.1",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "ignore-styles": "^5.0.1",
    "jsdom": "^8.5.0",
    "lint-staged": "^4.0.3",
    "mocha": "^4.0.1",
    "mock-css-modules": "^1.0.0",
    "postcss": "^6.0.9",
    "postcss-bem-fix": "^1.0.1",
    "postcss-calc": "^6.0.0",
    "postcss-color-function": "^4.0.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^6.1.0",
    "postcss-custom-selectors": "^4.0.1",
    "postcss-em-media-query": "^2.0.0",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-media-minmax": "^3.0.0",
    "postcss-nested": "^2.1.2",
    "postcss-nesting": "^4.0.1",
    "postcss-scss": "^1.0.2",
    "postcss-selector-matches": "^3.0.1",
    "postcss-url": "^7.1.2",
    "precss": "^2.0.0",
    "prettier": "^1.5.3",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-test-renderer": "^15.6.2",
    "redux-mock-store": "^1.3.0",
    "sinon": "^4.1.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack-dev-server": "^2.7.1"
  },
  "homepage": ""
}
